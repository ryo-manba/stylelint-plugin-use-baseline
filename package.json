{
  "name": "stylelint-plugin-use-baseline",
  "version": "1.0.0",
  "description": "A Stylelint plugin that enforces CSS feature availability based on Baseline.",
  "keywords": [
    "baseline",
    "css",
    "lint",
    "stylelint",
    "stylelint-plugin"
  ],
  "author": "ryo-manba",
  "license": "MIT",
  "homepage": "https://github.com/ryo-manba/stylelint-plugin-use-baseline#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryo-manba/stylelint-plugin-use-baseline"
  },
  "bugs": {
    "url": "https://github.com/ryo-manba/stylelint-plugin-use-baseline/issues"
  },
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "npm test --ignore-scripts -- --watch",
    "build": "rollup -c",
    "lint": "eslint .",
    "format": "prettier --write .",
    "validate": "npm run lint && npm run test",
    "build:baseline": "node tools/generate-baseline.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "css-tree": "^3.1.0",
    "is-plain-object": "^5.0.0",
    "postcss": "^8.5.5",
    "postcss-value-parser": "^4.2.0"
  },
  "peerDependencies": {
    "stylelint": "^16.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@stylelint/prettier-config": "^3.0.0",
    "common-tags": "^1.8.2",
    "compute-baseline": "^0.4.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.29.0",
    "eslint-config-stylelint": "^25.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^29.0.0",
    "jest": "^30.0.2",
    "jest-preset-stylelint": "^8.0.0",
    "mdn-data": "^2.24.0",
    "prettier": "^3.5.3",
    "rollup": "^4.43.0",
    "semantic-release": "^24.2.5",
    "stylelint": "^16.20.0",
    "web-features": "^3.1.0"
  }
}
